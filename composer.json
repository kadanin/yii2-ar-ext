{
  "name":              "kadanin/yii2-ar-ext",
  "type":              "library",
  "authors":           [
    {
      "name": "Kadanin"
    }
  ],
  "minimum-stability": "stable",
  "require":           {
    "php": ">=7.4",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev":           {
    "roave/infection-static-analysis-plugin": "^1.16",
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload":          {
    "psr-4": {
      "Kadanin\\Yii2ArExt\\": "src/",
      "tests\\": "tests/"
    }
  },
  "repositories":      [
    {
      "type": "composer",
      "url":  "https://asset-packagist.org"
    }
  ],
  "config":            {
    "allow-plugins": {
      "yiisoft/yii2-composer": false,
      "infection/extension-installer": true
    }
  }
}
